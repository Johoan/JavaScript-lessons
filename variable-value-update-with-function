function meal(animal) {
        animal.food = animal.food + 10;
    }

    var dog = {
        food: 10
    };
    meal(dog);
    meal(dog);

    console.log(dog.food);

//OUTPUT: *The food value starts at 10. The meal function is called twice which adds 10 to the food value each time. Therefore, 30 is printed.*
